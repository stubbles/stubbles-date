{
    "name": "stubbles/date",
    "type": "library",
    "homepage": "http://stubbles.net/",
    "license": "BSD-3-Clause",
    "description": "Simplified handling of dates and datespans.",
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6",
        "stubbles/core": "~5.5"
    },
    "autoload": {
        "psr-4": {
            "stubbles\\date\\": "src/main/php"
        },
        "files": ["src/main/php/functions.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.3.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
